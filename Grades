def school(marks):
    average=sum(marks)/len(marks)
    return average

def compute(average):
    if average > 80:
        Grade = "A"
    elif 60 < average <= 80:
        Grade = "B"
    elif 50 <= average <= 60:
        Grade = "C"
    else:
        Grade = "Fail"
    return Grade

marks=[10,67,98,45,39]

average=school(marks)
Grade=compute(average)
print("the averagr marks are",average)
print("the grade is ",Grade)
